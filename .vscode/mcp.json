{
	"servers": {
		"terraform": {
			"command": "sh",
			"args": [
				"-c",
				"docker run -i --rm -e TFE_TOKEN=\"$TF_TOKEN_app_terraform_io\" -e TFE_HOSTNAME=app.terraform.io hashicorp/terraform-mcp-server:0.3.0"
			]
		},
		"azure": {
			"command": "npx",
			"args": [
				"-y",
				"@azure/mcp@latest",
				"server",
				"start"
			],
			"env": {
				"AZURE_SUBSCRIPTION_ID": "fc62f8fe-7567-40ac-b199-a74f08b1386c"
			}
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		},
		"microsoftdocs": {
			"type": "http",
			"url": "https://learn.microsoft.com/api/mcp"
		},
		"context7": {
			"type": "http",
			"url": "https://mcp.context7.com/mcp",
			"headers": {
				"undefined": "{context7_api_key}"
			}
		}
	}
}
